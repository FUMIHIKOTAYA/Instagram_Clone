<div class="favorite_picture">
  <% @favorites.each do |favorite| %>

  <% if favorite.user.id == current_user.id %>
    <%= image_tag favorite.picture.image.url %>
    <%= favorite.picture.content %>
    <%= favorite.picture.user.name %>
    <%= link_to %q(お気に入り解除), favorite_path(favorite.id), method: :delete %>
  <% end %>
  
  <% end %>
</div>
